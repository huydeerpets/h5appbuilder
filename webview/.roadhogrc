{
  "entry": "src/index.js",
  "outputPath": "../buildserver/html",
  "publicPath": "/html/",
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "proxy": {
    "/api/v2/*": {
      "target":"http://localhost:8080/",
      "secure": false
    }
  }
}
